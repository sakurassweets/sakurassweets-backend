msgid "Email domain"
msgstr "Імʼя домену"

#: .\components\user\services\email_validation.py:126
msgid "Domain should not contain an underscore."
msgstr "Домен не повинен мати нижнього тире."

#: .\components\user\services\email_validation.py:139
msgid "Email domain name"
msgstr "Імʼя домену"

#: .\components\user\services\email_validation.py:154
msgid "Email contains some unallowed special characters."
msgstr "Email має недозволенні спеціальні символи."

#: .\components\user\services\email_validation.py:156
msgid "Email should not contain unallowed special characters."
msgstr "Email не повинен мати недозволенні спеціальні символи."

#: .\components\user\services\email_validation.py:182
msgid "Domain address should not contain any special characters"
msgstr "Доменна адреса не може містити будь-які спеціальні символи."

#: .\components\user\services\email_validation.py:183
msgid "Domain address should not contain any digits"
msgstr "Доменна адреса не може містити будь-які числа."

#: .\components\user\services\email_validation.py:184
msgid "Domain address should contain at least 2 characters"
msgstr "Доменна адреса повинна складатись як мінімум з 2 символів."

#: .\components\user\services\email_validation.py:185
msgid "Domain address should contain maximum 6 characters"
msgstr "Доменна адреса може мати максимум 6 символів."

#: .\components\user\services\email_validation.py:186
msgid ""
"Email domain address should not contain special characters, any digits, "
"should contain at least 2 characters and maximum 6 characters"
msgstr ""
"Доменна адреса не повинна мати будь-які спеціальні символи, будь-які цифри "
"та повинна мати від 2 до 6 символів."

#: .\components\user\services\email_validation.py:187
msgid "Email domain address"
msgstr "Імʼя доменного адресу"

#: .\components\user\services\email_validation.py:250
#, python-format
msgid "%(object)s can't start or end with '%(symbols)s' symbols."
msgstr ""
"%(object)s не можуть починатись або закінчуватись символами '%(symbols)s'."

#: .\components\user\services\email_validation.py:251
#, python-format
msgid "%(object)s should have at least one non-digit character."
msgstr "%(object)s повинно мати хоча б один не числовий символ."

#: .\components\user\services\email_validation.py:252
#, python-format
msgid "%(object)s shouldn't start or end with '%(symbols)s' symbols."
msgstr ""
"%(object)s не можуть починатись або закінчуватись символами '%(symbols)s'."

#: .\components\user\services\email_validation.py:294
#, python-format
msgid "Your email have %(value)d '@' symbols, only 1 allowed."
msgstr "Ваш email має %(value)d символи '@', але лише 1 дозволенно"

#: .\components\user\services\email_validation.py:295
msgid "Your email doesn't have any '@' symbol."
msgstr "Ваш email немає жодного символу '@'."

#: .\components\user\services\email_validation.py:296
msgid "Domain should contain domain name and domain address splitted by dot"
msgstr ""
"Домен повинен складатись з доменного імʼя та доменної адреси розділених "
"крапкою."

#: .\components\user\services\email_validation.py:297
#, python-format
msgid "You'r %(object)s contains too many special characters in a row"
msgstr "Ваше %(object)s має забагато спеціальних підряд."

#: .\components\user\services\email_validation.py:298
msgid "Your email should have only 1 '@' symbol"
msgstr "Ваш email повинен мати лише 1 символ '@'."

#: .\components\user\services\password_validation.py:29
#: .\components\user\validators.py:63
msgid "Password should be not None."
msgstr "Пароль не може бути None."

#: .\components\user\services\password_validation.py:123
msgid "Only latin characters allowed in password"
msgstr "Лише латинські літери дозволенні у паролі."

#: .\components\user\services\password_validation.py:124
msgid "Password must contain only latin characters"
msgstr "Пароль може містити лише латинські літери."

#: .\components\user\services\password_validation.py:170
#, python-format
msgid ""
"Password must be at least %(value)s characters long.Got %(len)s instead."
msgstr ""
"Пароль повинен мати як мінімум %(value)s символів. Отримано лише %(len)s символів."

#: .\components\user\services\password_validation.py:171
#, python-format
msgid ""
"Password must be shorter than %(value)s characters long.Got %(len)s instead."
msgstr ""
"Пароль повинен бути коротший %(value)s символів. Отримано %(len)s символів."

#: .\components\user\services\password_validation.py:172
#, python-format
msgid ""
"Your password should have minimum %(min_len)d characters and maximum "
"%(max_len)d characters"
msgstr ""
"Ваш пароль повинен мати від %(min_len)d до %(max_len)d символів"

#: .\components\user\services\password_validation.py:209
#, python-format
msgid ""
"Password must contain at least %(min_upper)d uppercase letter and "
"%(min_lower)d lowercase letter."
msgstr ""
"Пароль повинен мати як мінімум %(min_upper)d символ верхнього регістру та "
"%(min_lower)d символ нижнього регістру."

#: .\components\user\services\password_validation.py:210
#, python-format
msgid ""
"Your password must contain at least %(min_upper)d uppercase letter and "
"%(min_lower)d lowercase letter."
msgstr ""
"Пароль повинен мати як мінімум %(min_upper)d символ верхнього регістру та "
"%(min_lower)d символ нижнього регістру."

#: .\components\user\services\password_validation.py:231
msgid "Password should not contain any spacebars."
msgstr "Пароль не може містити пробіли."

#: .\components\user\services\password_validation.py:232
msgid "Your password should not contain any spacebars."
msgstr "Ваш пароль не повинен містити пробіли."

#: .\components\user\services\password_validation.py:243
#, python-format
msgid "Password should contain at least %(value)d digit."
msgstr "Пароль повинен містити як мінімум %(value)d цифру."

#: .\components\user\services\password_validation.py:244
#, python-format
msgid "You`r password should contain at least %(value)d digit."
msgstr "Ваш пароль повинен містити як мінімум %(value)d цифру."

#: .\components\user\validators.py:50
msgid "Email should be not None."
msgstr "Email не може бути None."

